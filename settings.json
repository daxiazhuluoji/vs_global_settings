{
    "vim.disableAnnoyingNeovimMessage": true,
    "editor.minimap.enabled": false,
    "workbench.statusBar.feedback.visible": false,
    "window.title": "${dirty}${activeEditorLong}${separator}${rootName}${separator}${appName}",
    "window.menuBarVisibility": "toggle",
    "workbench.sideBar.location": "left",
    "editor.renderWhitespace": "none",
    "search.location": "panel",
    "workbench.statusBar.visible": true,
    "workbench.iconTheme": null,
    "workbench.activityBar.visible": true,
    "explorer.openEditors.visible": 2,
    "editor.lineNumbers": "on",
    "workbench.colorTheme": "DesertEx",
    "editor.formatOnType": true,
    "editor.formatOnPaste": true,
    "window.zoomLevel": 0,
    "terminal.integrated.shell.windows": "D:\\cygwin64\\bin\\bash.exe",
    "files.exclude": {
        "**/*.htm": true,
        "**/backup": true
    },
    "search.useIgnoreFiles": false,
    "fileheader.tpl": "/*\r\n * @Author: {fileName} \r\n * @Author: {author} \r\n * @Date: {createTime} \r\n * @Last Modified by:   {lastModifiedBy} \r\n * @Last Modified time: {updateTime} \r\n */\r\n",
    "fileheader.Author": "ldj",
    "fileheader.LastModifiedBy": "ldj",
    "editor.fontSize": 16,
    "editor.mouseWheelZoom": true
}